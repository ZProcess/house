<struts>
	<package name="senduser" extends="json-default">
		<action name="sendusercrud" class="com.softxm.hs.action.SendUserAction">
			<result name="addSendUser" type="json">
				<param name="includeProperties">result</param>
			</result>
			<result name="sendUserList">/main/sendUserjsp/sendUserList.jsp</result>
			<result name="center">/common/message.jsp</result>
